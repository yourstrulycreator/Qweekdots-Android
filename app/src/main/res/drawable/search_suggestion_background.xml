<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="@color/tenor_sdk_primary_dark_color" />
        </shape>
    </item>
    <item android:id="@+id/ssb_drawable_background" android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="@color/tenor_sdk_primary_color" />
        </shape>
    </item>
</selector>
